<div class="portletBody">
#if ( $failures )
<p>$tlang.getString("contentitem.failures")</p>
<ul>
#foreach($failure in $failures)
<li>$failure</li>
#end
</ul>
#else
<script>includeLatestJQuery('lti_assignment_return.vm');</script>
<script>
